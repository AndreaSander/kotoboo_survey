evidence <- x_2022_may_qualtrics_data %>%
select(how_helpful_evidence_based)
evidence<- as.data.frame(table(evidence$how_helpful_evidence_based))
evidence<- evidence %>%
mutate(percentage = (Freq/110)*100)
evidence <- x_2022_may_qualtrics_data %>%
select(how_helpful_evidence_based)
evidence_table<- as.data.frame(table(evidence$how_helpful_evidence_based))
evidence_table <- evidence_table %>%
mutate(percentage = (Freq/110)*100)
